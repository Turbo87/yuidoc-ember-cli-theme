      <thead>
      <tr>
        <th colspan="2">Protected Methods</th>
      </tr>
      </thead>
      <tbody>
      {{#each (filter methods "!static" "protected")}}
        {{>method-short}}
      {{/each}}
      </tbody>
    </table>
  {{/if}}

  {{#if (filter methods "!static" "private")}}
    <table class="summary">
      <thead>
      <tr>
        <th colspan="2">Private Methods</th>
      </tr>
      </thead>
      <tbody>
      {{#each (filter methods "!static" "private")}}
        {{>method-short}}
      {{/each}}
      </tbody>
    </table>
  {{/if}}
{{/if}}

