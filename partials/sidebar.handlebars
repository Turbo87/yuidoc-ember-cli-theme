    </div>
</div>
{{/if}}

{{#if attributes}}
<div id="attrs-on-page" class="sidebox on-page">
    <div class="hd">
        <h2 class="no-toc">Attributes On Page</h2>
    </div>
    <div class="bd">
        <ul>
            {{#attributes}}
                <li><a href="#attrs_{{name}}">{{name}}</a></li>
            {{/attributes}}
        </ul>
    </div>
</div>
{{/if}}

<div class="version-info">
  Version: {{projectVersion}}
</div>

